<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/primaryLightColor</item>
    </style>

    <style name="DarkAppTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="colorPrimary">@color/primaryColor2</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor2</item>
        <item name="colorAccent">@color/primaryLightColor2</item>
    </style>
</resources>
